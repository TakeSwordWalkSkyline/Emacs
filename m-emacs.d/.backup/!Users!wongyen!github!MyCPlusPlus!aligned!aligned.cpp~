#include <stdio.h>

struct A {
    char a;                          // 1 bytes
    char *b;                         // 8 bytes
    int c() { return 0; }            // 0 bytes
    virtual void CommonFunction(){}; // 8 bytes
   };
//__attribute__((aligned (8)));

int main() {
    A test;
    printf("sizeof %lu\n", sizeof(test));
    return 0;
}
